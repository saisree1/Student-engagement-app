</head>
<body>
<div style="" class="d1">
 <label style="" class="k2">Student Engagement Application</label>
  
       </div>

<%var found=false%>
      
      
      <% for(var i=0;i<stu.length ;i++){%>
      <%=stu[i].Tname%>
      

      <% for(var k=0;k<res.length;k++){%>
      <% for(var l=0;l<res[k].courses.length;l++){%>
      <%if(stu[i].cid==res[k].courses[l]){%>
        <% for(var j=0;j<stu[i].areg.length ;j++){%>
        <%if(stu[i].areg[j+1]==res[k]._id){%>
         <%var found=true%>
        
         <%break%>
         <%}else{%>
         
         <%var found=false%>
         <%}%>

        <%}%>
        <%if(found){%>
          <form>
       <div class="row" style=""> 
                <div class="col-md-4" style="">   
      <label>Reg.no:--></label><input type="text" style="width:45%;margin-top:1%" name="areg" value="<%=stu[i].areg[j+1]%>" readonly><br>
                        
        <label>Marks:--></label><input type="text" style="width:15%;margin-top:1%" pattern="[0-9]{2}" 
        name="tmarks" value="<%=stu[i].tmarks[j+1]%>" disabled>

        
        </form>  
         </div>
         </div>
        <%}else{%>
         <form>
       <div class="row" style=""> 
                <div class="col-md-4" style="">   
      <label>Reg.no:--></label><input type="text" style="width:45%;margin-top:1%" name="areg" value="<%=res[k]._id%>" readonly><br>
                        
        <label>Marks:--></label><input type="text" style="width:15%;margin-top:1%" pattern="[0-9]{2}" 
        name="tmarks" value="">

        <button class="btn orange" value="submit" formaction="/add_test/<%=stu[i].Tname%>">Submit</button>
        </form>  
         </div>
         </div>
        <%}%>
      <%}else{%>
         
      <%}%>
      <%}%>
      <%}%>

      
      <%}%>
           
      
                
           
          
 
